<h1> Login </h1>

<%= form_with scope: @user, url: users_path, local: true do |form| %>
    <p>
      <%= form.label :Name %><br>
      <%= form.text_field :User_name %>
    </p>
   
    <p>
      <%= form.label :Email %><br>
      <%= form.text_field :email %>
    </p>
   
    <p>
      <%= form.submit "Login"%>
    </p>
  <% end %>